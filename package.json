  "main": "main.js",
  "homepage": "https://dinh0012.github.io/react/",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "extract-intl": "node -r esm ./internals/scripts/extract-intl.js",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "node ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "cross-env plop --plopfile generators/plopfile.ts",
    "lint": "npm run typecheck && npm run lint:css && npm run lint:eslint:quiet .",
    "lint:css": "stylelint './app/**/*.{js,ts,tsx}'",
    "lint:eslint:quiet": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --quiet",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:eslint:fix": "eslint --ext .ts,.tsx --no-error-on-unmatched-pattern --quiet --fix",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write  --ignore-path ./.prettierignore"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write  --ignore-path ./.prettierignore",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged && npm run typecheck"
    }
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
